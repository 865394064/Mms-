<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="TextAppearance">
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textColorHighlight">#FFFF9200</item>
        <item name="android:textColorHint">?android:textColorHint</item>
        <item name="android:textColorLink">#5C5CFF</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="TextAppearance.SearchResult">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimaryInverse</item>
        <item name="android:textColorHint">?android:textColorHintInverse</item>
    </style>

    <style name="TextAppearance.SearchResult.Title">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TextAppearance.SearchResult.Subtitle">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">?android:textColorSecondaryInverse</item>
    </style>

    <style name="MmsHoloTheme" parent="@style/ThemeNoTitleBar">
    </style>
     <style name="ThemeNoTitleBar" parent="android:Theme.NoTitleBar">
        <!-- Begin: modidifed by lzp on 20130910 -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowAnimationStyle">@null</item>
        <!-- End: modidifed by lzp on 20130910 --> 
    </style>

     <style name="WidgetTitle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#333333</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="NotificationPrimaryText">
        <item name="android:textColor">#cccccc</item>
    </style>

    <style name="NotificationSecondaryText">
        <item name="android:textColor">#666666</item>
    </style>

    <style name="NotificationSubjectText">
        <item name="android:textColor">#999999</item>
    </style>
 
    <style name="MultiSelectionHoloTheme" parent="@android:style/Theme.Holo.Light">
		<item name="android:actionBarStyle">@style/MultiSelectionActionBarStyle</item>
	</style>
	
	<style name="MultiSelectionActionBarStyle" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/cab_background_top_holo_dark</item>
        <item name="android:backgroundStacked">@drawable/cab_background_top_holo_dark</item>
        <item name="android:backgroundSplit">@drawable/cab_background_bottom_holo_dark</item>
        <item name="android:itemPadding">9dip</item>
    </style>
  <!-- Dialog mode-->
  <style name="Theme.SmsDlgScreen" parent="@android:style/Theme.Holo.Light.Dialog">
      <item name="android:windowNoTitle">true</item>
  </style>

    <!-- IpMessage -->
    <style name="out_going_time_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingRight">4dip</item>
        <item name="android:textColor">#555555</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="out_going_type_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">3dip</item>
        <item name="android:paddingRight">3dip</item>
        <item name="android:layout_marginRight">4dip</item>
        <item name="android:textColor">#555555</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">10sp</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:maxWidth">112dip</item>
    </style>

    <style name="separator_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dip</item>
        <item name="android:background">@drawable/ipmsg_chat_split_line</item>
    </style>

    <style name="bubble_name_separator_style" parent="@style/separator_style">
        <item name="android:layout_marginBottom">9dp</item>
    </style>

    <style name="caption_separator_style" parent="@style/bubble_name_separator_style">
        <item name="android:layout_marginBottom">6dp</item>
        <item name="android:layout_marginTop">6dp</item>
    </style>

    <style name="group_member_name_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">27dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#5a5a5a</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="MmsHoloThemeWhite" parent="@style/MmsHoloTheme">
        <item name="android:actionBarWidgetTheme">@style/HoloLightSelectMode</item>
    </style>
    <style name="HoloLightSelectMode" parent="android:style/Widget.Holo.ListPopupWindow">
        <item name="android:popupBackground">@null</item>
    </style>

    <!-- M: media controller style @{-->
    <style name="MediaButton">
        <item name="android:background">@null</item>
        <item name="android:layout_width">79dip</item>
        <item name="android:layout_height">58dip</item>
    </style>

    <style name="MediaButton.Previous">
        <item name="android:src">@drawable/ic_media_pre</item>
    </style>

    <style name="MediaButton.Next">
        <item name="android:src">@drawable/ic_media_next</item>
    </style>

    <style name="MediaButton.Play">
        <item name="android:src">@drawable/ic_media_play</item>
    </style>

    <style name="MediaButton.Ffwd">
        <item name="android:src">@drawable/ic_media_ff</item>
    </style>

    <style name="MediaButton.Rew">
        <item name="android:src">@drawable/ic_media_rew</item>
    </style>

    <style name="MediaButton.Pause">
        <item name="android:src">@drawable/ic_media_pause</item>
    </style>
    <!-- @}-->
    <!--靠左普通圆角按钮深蓝色-->
	<style name="IPhone_Button_Left_DarkBLue_Style" parent="style/IPhone_Button_Style">
		<item name="android:background">@android:color/transparent
		</item>
	</style>  
    <!--普通圆角按钮-->
	<style name="IPhone_Button_Style" parent="@android:style/Widget.Button">
		<item name="android:textSize">15dp</item>
		<item name="android:textStyle">normal</item>
		<item name="android:textColor">@drawable/iphone_btn_blue_color</item>
		<item name="android:gravity">center</item>
		<item name="android:singleLine">true</item>
		<item name="android:ellipsize">end</item>
		<item name="android:maxWidth">112dip</item>
	</style>
    <!-- 标题TextView风格 -->
	<style name="Header_TextView_Style" parent="@android:style/Widget.TextView">
		<item name="android:textColor">#000000
		</item>
		<item name="android:background">@android:color/transparent
		</item>
		<item name="android:textSize">15dp</item>
		<item name="android:layout_centerHorizontal">true</item>
		<item name="android:layout_centerVertical">true</item>
		<item name="android:ellipsize">end</item>
		<item name="android:maxWidth">157dip</item>
		<item name="android:singleLine">true</item>
	</style>
    <!--手写图标Button（如新建短信按钮）  -->
	<style name="IPhone_Button_Handwrite_Style" parent="style/IPhone_Button_Style">
		<item name="android:background">@drawable/iphone_button_handwrite_bg    
		</item>
	</style>
       <!-- 指向左箭头形状深蓝色按钮（功能为“返回”的按钮） -->
	<style name="IPhone_Left_Arrow_Shape_Button_Style" parent="style/IPhone_Button_Style">
		<item name="android:background">@drawable/iphone_button_arraw_bg
		</item>
	</style>
       <!--靠右普通圆角按钮深蓝色-->
	<style name="IPhone_Button_Right_DarkBLue_Style" parent="style/IPhone_Button_Style">
		<item name="android:background">@android:color/transparent
		</item>
	</style>
    <!--靠右普通圆角按钮浅黑色-->
	<style name="IPhone_Button_Right_LightBlue_Style" parent="style/IPhone_Button_Style">
		<item name="android:background">@drawable/iphone_button_right_bg_1
		</item>
	</style>
    <style name="MinusButton">
        <item name="android:background">@drawable/gallery_delete_small_bg</item>
    </style>
    <!-- Begin : added by tf -->
    <style name="mystyle" parent="android:Theme">
		<item name="android:windowTitleSize">50dp</item>
	</style>
	<!-- End : added by tf -->

    
</resources>
